      <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card ">
                <div id="drop" align=center></div>
                
                <div id="bar-graph">
                    </br>
                    <h5 id="bar-graph-title" class="title"></h5> 
                </div>
              
            <script>                

                // set the dimensions and margins of the graph
                var margin = {top: 20, right: 20, bottom: 30, left: 100},
                    width = 800 - margin.left - margin.right,
                    height = 500 - margin.top - margin.bottom;

                // Set the ranges
                var x = d3.scaleBand().rangeRound([0, width]).padding(0.1)
                var y = d3.scaleLinear().range([height, 0]);

                // define the line
                var valueline = d3.line()
                    .x(function(d) { return x(d.device_type); })
                    .y(function(d) { return y(d.click); });
                
                // Create the svg canvas in the "graph" div
                var svg = d3.select("#bar-graph")
                        .append("svg")
                        .style("width", width + margin.left + margin.right + "px")
                        .style("height", height + margin.top + margin.bottom + "px")
                        .attr("width", width + margin.left + margin.right)
                        .attr("height", height + margin.top + margin.bottom)
                        .append("g")
                        .attr("transform","translate(" + margin.left + "," + margin.top + ")")
                        .attr("class", "svg");

                // Get the data
                d3.csv("1200_sample.csv", function(error, data) {
                    if (error) throw error;
                    for (var i = 0; i < 5; i++) {
//                        console.log(data[i].day_of_week);
//                        console.log(data[i].Age);
                    }
                    
                    // Groupby device type and then sum clicks
                    var clicksTotal = d3.nest()
                      .key(function(d) { return d.device_type; }).sortKeys(d3.ascending)
                      .rollup(function(v) { return d3.sum(v, function(d) { return d.click; }); })
                      .entries(data);
//                    console.log(JSON.stringify(clicksTotal));
                    
                     // Scale the range of the data
                      x.domain(clicksTotal.map(function(d) { return d.key; }));
                      y.domain([0, d3.max(clicksTotal, function(d) { return d.value; })]);
                    
                    // Scale y?
//                    var y = d3.scale.linear()
//                        .domain([0, d3.max(data, function(d){
//                            return +d[selection];
//                        })])
//                        .range([height, 0]);

                    // Set up the x axis
                      var xaxis = svg.append("g")
                           .attr("transform", "translate(0," + height + ")")
                           .attr("class", "x axis")
                           .call(d3.axisBottom(x)
                              //.ticks(d3.timeMonth)
                              .tickSize(0, 0)
                              //.tickFormat(d3.timeFormat("%B"))
                              .tickSizeInner(0)
                              .tickPadding(10));

                      // Add the Y Axis
                       var yaxis = svg.append("g")
                           .attr("class", "y axis")
                           .call(d3.axisLeft(y)
                              .ticks(10)
                              .tickSizeInner(0)
                              .tickPadding(6)
                              .tickSize(0, 0));

                       // Draw the bars
                      svg.selectAll(".rect")
                          .data(clicksTotal)
                          .enter()
                          .append("rect")
                              .attr("class", "bar")
                              .attr("x", function(d) { return x(d.key); })
                              .attr("y", function(d) { return y(d.value); })
                              .attr("width", x.bandwidth())
                              .attr("height", function(d) { return height - y(d.value); });

                       var chartTitle = "Clicks by Device";
                         d3.select("#bar-graph-title")
                         .text(chartTitle);

                    	// Get every column value - DONE
                        var elements = Object.keys(data[0])
//                            .filter(function(d){
//                                return ((d != "Year") & (d != "State"));
//                            });
                        var selection = elements[0];
                        console.log(elements);

                        var selector = d3.select("#drop")
                        .append("select")
                        .attr("id","dropdown")
                        .on("change", function(d){
                            selection = document.getElementById("dropdown");

                            y.domain([0, d3.max(data, function(d){
                                return +d[selection.value];})]);

                            yAxis.scale(y);

                            d3.selectAll(".rectangle")
                                .transition()
                                .attr("height", function(d){
                                    return height - y(+d[selection.value]);
                                })
                                .attr("x", function(d, i){
                                    return (width / data.length) * i ;
                                })
                                .attr("y", function(d){
                                    return y(+d[selection.value]);
                                })
                                .ease("linear")
                                .select("title")
                                .text(function(d){
                                    return d.State + " : " + d[selection.value];
                                });

                            d3.selectAll("g.y.axis")
                                .transition()
                                .call(yAxis);

                         });
                    
                        selector.selectAll("option")
                          .data(elements)
                          .enter().append("option")
                          .attr("value", function(d){
                            return d;
                          })
                          .text(function(d){
                            return d;
                          })
              });

                </script>
                
            </div>
          </div>
